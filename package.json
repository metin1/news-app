    "concurrently": "^6.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "joi": "^17.4.0",
    "nodemon": "^2.0.7",
    "path": "^0.12.7",
    "supertest": "^6.1.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "jest": "26.6.0"
