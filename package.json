{
  "name": "coffee-project",
  "version": "1.0.0",
  "description": "This Application List of Coffees, filter by coffee categories and getting search result. ",
  "main": "server.js",
  "scripts": {
    "test": "jest --coverage --logHeapUsage --maxWorkers=2 --testPathPattern=__tests__/unit",
